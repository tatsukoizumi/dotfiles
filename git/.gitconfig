[alias]
  co = checkout
  sw = switch
  cm = commit
  st = status
  br = branch
  pl = pull
  ps = push
  ac = !git add -A && git commit
  ame = commit --amend --no-edit
  branches = branch -a
  tags = tag -l
  f = fetch --prune
  undo = reset HEAD~1 --mixed
  unstage = reset HEAD -- .
  wt = worktree

[push]
  default = current

[include]
  path = ~/.gitconfig.local
